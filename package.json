{
    "name": "panorama-css",
    "displayName": "Panorama CSS Support",
    "description": "Support for the Panorama subset of CSS3.",
    "version": "0.2.1",
    "publisher": "braemie",
    "author": {
        "name": "Brae Stoltz",
        "email": "brae@momentum-mod.org",
        "url": "https://github.com/braem"
    },
    "icon": "icon.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.28.0"
    },
    "contributes": {
        "grammars": [
            {
                "language": "css",
                "scopeName": "panoramasource.css",
                "path": "./syntaxes/panoramacss.tmLanguage.json"
            }
        ],
        "css": {
            "customData": [ "./data/panorama.css-data.json" ]
        },
        "snippets": [
            {
                "language": "css",
                "path": "./snippets/-s2-mix-blend-mode.json"
            },
            {
                "language": "css",
                "path": "./snippets/gradient.json"
            },
            {
                "language": "css",
                "path": "./snippets/flow-children.json"
            },
            {
                "language": "css",
                "path": "./snippets/misc.json"
            }
        ]
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "panorama",
        "css"
    ],
    "bugs": {
        "url": "https://github.com/braem/vscode-panorama-css/issues",
        "email": "brae@momentum-mod.org"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/braem/vscode-panorama-css"
    },
    "homepage": "https://github.com/braem/vscode-panorama-css"
}
